{
    "name": "aura/includer",
    "type": "library",
    "description": "Include multiple files from specified directories, in order, with variables extracted into a limited include scope.",
    "keywords": [
        "include",
        "limited",
        "scope"
    ],
    "homepage": "https://github.com/auraphp/Aura.Includer",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Aura.Includer Contributors",
            "homepage": "https://github.com/auraphp/Aura.Includer/contributors"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "Aura\\Includer\\": "src/"
        }
    },
    "extra": {
        "aura": {
            "type": "library"
        }
    }
}
